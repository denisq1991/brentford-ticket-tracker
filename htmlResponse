<script>$(document).ready(function () {
        $(document).ready(function () {


            $('#custom-sidebar-nav ul ul li:odd').addClass('odd');
            $('#custom-sidebar-nav ul ul li:even').addClass('even');
            $('#custom-sidebar-nav > ul > li > a').click(function () {
                $('#custom-sidebar-nav li').removeClass('active');
                $(this).closest('li').addClass('active');
                var checkElement = $(this).next();
                if ((checkElement.is('ul')) && (checkElement.is(':visible'))) {
                    $(this).closest('li').removeClass('active');
                    checkElement.slideUp('normal');
                }
                if ((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
                    $('#custom-sidebar-nav ul ul:visible').slideUp('normal');
                    checkElement.slideDown('normal');
                }
                if ($(this).closest('li').find('ul').children().length == 0) {
                    return true;
                } else {
                    return false;
                }
            });

        });

    });</script>
